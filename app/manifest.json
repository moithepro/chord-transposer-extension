{
    "manifest_version": 3,
    "name": "Simple Chord transposer",
    "version": "1.0",
    "description": "A simple Chrome extension with +/- buttons.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ]
  }